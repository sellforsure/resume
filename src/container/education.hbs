<div>
    {{#with main}}
    <main>
        <section>
            {{>title data="My education" big=true}}
            <ul>
                {{#each educations}}
                <li>
                    {{name}}
                    {{#unless isEnd}}
                    (ще в процесі)
                    {{/unless}}
                </li>
                {{/each}}
            </ul>
        </section>

        <section>
            {{>title data="My certificates" big=false}}
            <ul>
                {{#each certificates}}
                <li>
                    {{name}}
                    {{#if id}} ({{id}}) {{/if}}
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>